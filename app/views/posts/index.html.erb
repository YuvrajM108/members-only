<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th>Post</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.post %></td>
        <td><%= link_to 'Show', posts_path %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "Logout", destroy_user_session_path, method: :delete, class:"button is-info" %>

<br>

<%= link_to 'New Post', new_post_path %>
